{
	"name": "meta-box/mb-pods-migration",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress",
		"wordpress-plugin",
		"types",
		"pods",
		"meta-box",
		"custom-fields",
		"custom-field"
	],
	"description": "Migrate Pods custom fields to Meta Box",
	"homepage": "https://metabox.io",
	"license": "GPL-2.0",
	"autoload": {
		"psr-4": {
			"MetaBox\\Pods\\": "src/"
		}
	},
	"config": {
		"prepend-autoloader": false
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/wpmetabox/support.git"
		},
		{
			"type": "git",
			"url": "https://github.com/wpmetabox/mbb-parser.git"
		}
	],
	"require": {
		"meta-box/support": "dev-master",
		"meta-box/mbb-parser": "dev-master"
	}
}
